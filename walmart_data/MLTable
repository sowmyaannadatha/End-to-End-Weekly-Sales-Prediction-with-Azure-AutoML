paths:
  - file: ./walmart.csv
transformations:
  - read_delimited:
      delimiter: ","
      encoding: "utf8"
      header: true

schema:
  store: string
  dept: integer
  date: datetime
  isholiday: boolean
  day_of_week: integer
  month: integer
  year: integer
  week_of_year: integer
  rolling_7: float
  rolling_30: float

